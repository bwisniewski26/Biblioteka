@page "/refresh"
@page "/refresh/{text}"
@page "/refresh/{text}/{id}"
@inject NavigationManager Navigator

@code {
    [Parameter]
    public string Text { get; set; } = "";
    [Parameter]
    public string id { get; set; } = "";

    protected override void OnInitialized()
    {
        if (Text == "accountmanager")
        {
            Navigator.NavigateTo("/accountmanager/"+id);
            return;
        }
        Navigator.NavigateTo("/account/"+Text);
    }

}